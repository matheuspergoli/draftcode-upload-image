{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "dist"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
